{
  "name": "elitefantasybasketball-server",
  "version": "0.0.1",
  "description": "",
  "main": "server-controller.ts",
  "scripts": {
    "precompile": "tsc",
    "start-server": "NO_COLOR=true tsc && mkdir ./dist/logs/ && touch ./dist/logs/server.out && cp ./deferred-response-handler/web-component-to-image-builder/web-components/svelte/src/LiveStandings.svelte ./dist/deferred-response-handler/web-component-to-image-builder/web-components/svelte/src/LiveStandings.svelte && nohup node ./dist/server-controller.js > ./dist/logs/server.out 2>&1 &",
    "stop-server": "rm -r /home/efb-admin/src/dist/ && rm server.lock && pkill -f server-controller.js",
    "check-server": "SERVER_PID=$(pgrep -f server-controller.js | grep -v $$); if [ -z \"$SERVER_PID\" ]; then echo 'Server is not running'; else echo \"Server is running with process ID $SERVER_PID\"; fi",
    "restart-server": "npm run stop-server && npm run start-server",
    "deploy-commands": "tsc && cp ./discord-command-handler/deploy-commands.js ./dist/discord-command-handler/deploy-commands.js && node --require dotenv/config ./dist/discord-command-handler/deploy-commands.js",
    "test-web-components": "vite",
    "build-web-components": "vite build",
    "preview-web-components": "vite preview",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "author": "milesfriedmanfl",
  "license": "",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.262.0",
    "@aws-sdk/client-efs": "^3.279.0",
    "@aws-sdk/lib-dynamodb": "^3.410.0",
    "@discordjs/rest": "^1.5.0",
    "@nestjs/common": "^10.3.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/platform-express": "^10.3.1",
    "@types/node": "^20.11.0",
    "aws-sdk": "^2.1363.0",
    "discord-api-types": "^0.37.35",
    "discord-interactions": "^3.2.0",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "node-fetch": "^3.3.0",
    "node-html-to-image": "^4.0.0",
    "rxjs": "^7.8.0",
    "winston": "^3.10.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "eslint": "^8.32.0",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  },
  "type": "module"
}
